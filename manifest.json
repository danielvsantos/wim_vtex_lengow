{
  "vendor": "webimpacto",
  "name": "wimvtexlengow",
  "version": "0.1.2",
  "title": "Lengow Integration",
  "description": "Integrate sync orders from Lengow flow",
  "credentialType": "absolute",
  "defaultLocale": "es-AR",
  "builders": {
    "graphql": "1.x",
    "react": "2.x",
    "pages": "0.x",
    "node": "3.x",
    "messages": "0.x"
  },
  "mustUpdateAt": "2019-08-24",
  "categories": [],
  "registries": [
    "smartcheckout"
  ],
  "dependencies": {
    "vtex.admin": "1.x",
    "vtex.styleguide": "8.x",
    "vtex.store-graphql": "2.x"
  },
  "billingOptions": {
    "free": true
  },
  "settingsSchema": {},
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "policies": [
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/catalog_system/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/vlm/account/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/XMLData/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs":{
        "host": "{{account}}.vtexpayments.com.br",
        "path":"*"
      }
    },
    {
      "name": "outbound-access",
      "attrs":{
        "host": "{{account}}.myvtex.com",
        "path":"*"
      }
    },
    {
      "name": "outbound-access",
      "attrs":{
        "host": "api.lengow.io",
        "path":"*"
      }
    },
    {
      "name": "outbound-access",
      "attrs":{
        "host": "api.lengow.net",
        "path":"*"
      }
    },
    {
      "name": "Get_Account_By_Identifier"
    }
  ]
}